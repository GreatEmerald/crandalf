package: fftw
recipe: sudo apt-get install -qq libfftw3-dev > /dev/null

package: gmp
recipe: sudo apt-get install -qq libgmp-dev > /dev/null

package: gsl
recipe: sudo apt-get install -qq libgsl0-dev > /dev/null

package: rmarkdown
recipe: ~/R/crandalf/scripts/install-pandoc

package: optiRum
recipe: sudo apt-get -qq install texlive-latex-base texlive-xetex texlive-latex-recommended texlive-fonts-extra > /dev/null

package: RAppArmor
recipe: sudo apt-get -qq install libapparmor-dev > /dev/null

package: RcppOctave
recipe: sudo apt-add-repository -y ppa:octave/stable;
  sudo apt-get -qq update;
  sudo apt-get install -qq octave liboctave-dev > /dev/null

package: Rglpk
recipe: sudo apt-get install -qq libglpk-dev > /dev/null

package: Rmpfr
recipe: sudo apt-get install -qq libmpfr-dev > /dev/null

package: RProtoBuf
recipe: sudo apt-get install -qq libprotoc-dev > /dev/null

package: R.rsp
recipe: sudo apt-get -qq install texlive-latex-extra

package: rgdal
recipe: sudo apt-get install -qq libgdal-dev libproj-dev > /dev/null

package: rjags
recipe: sudo apt-get install jags

package: seewave
recipe: sudo apt-get install -qq libsndfile1-dev > /dev/null

package: survSNP
recipe: sudo apt-get install -qq libgsl0-dev > /dev/null
